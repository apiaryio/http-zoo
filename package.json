{
  "name": "http-zoo",
  "version": "0.1.0",
  "description": "HTTP edge cases test suite",
  "main": "index.js",
  "homepage": "https://github.com/apiaryio/http-zoo#readme",
  "author": "Michal Holasek <michal.holasek@apiary.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apiaryio/http-zoo.git"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "scripts": {
    "build": "npm run clean && webpack --config config/webpack.test.js",
    "clean": "rimraf dist",
    "start": "npm run build && node dist/server.js",
    "test:browser": "karma start config/karma.js",
    "test:node": "mocha test/index.js",
    "test": "concurrently \"npm run test:browser\" \"npm test:node\""
  },
  "dependencies": {
    "axios": "^0.15.3",
    "express": "^4.14.0",
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "mocha": true
    }
  }
